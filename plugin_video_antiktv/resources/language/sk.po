#: plugin_video_antiktv/atk_client.py:182
msgid "Others"
msgstr "Ostatné"

#: plugin_video_antiktv/atk_client.py:409
msgid "Wrong communication data received from server"
msgstr "Nesprávne komunikačné údaje prijaté zo servera"

#: plugin_video_antiktv/atk_client.py:446
msgid "Request to remote server failed"
msgstr "Požiadavka na vzdialený server zlyhala"

#: plugin_video_antiktv/atk_client.py:448
msgid "Login failed"
msgstr "Prihlásenie zlyhalo"

#: plugin_video_antiktv/atk_client.py:450
msgid "Request is blocked by firewall"
msgstr "Požiadavka blokovaná firewallom"

#: plugin_video_antiktv/atk_client.py:458
msgid "Unexpected return code from server"
msgstr "Neočakávaný návratový kód zo servera"

#: plugin_video_antiktv/atk_client.py:508
msgid "Wrong login data provided or account doesn't exists."
msgstr "Prihlasovacie údaje sú nesprávne alebo používateľský účet neexistuje."

#: plugin_video_antiktv/atk_client.py:513
msgid "Account is not yet activated. You need to activate it using instructions in SMS or E-mail entered by registration."
msgstr "Účet nie je aktivovaný. Je nutné ho aktivovať podľa inštrukcií v SMS alebo E-maily zadanom pri registrácii."

#: plugin_video_antiktv/atk_client.py:515
#, python-brace-format
msgid "You have reached the limit of maximum registered devices. Go to {my_antik}, section Devices and removed unused one."
msgstr "Dosiahli ste limit maximálneho počtu registrovaných zariadení. Choďte na {my_antik}, sekcia Zariadenia a vymažte nejaké nepoužívané."

#: plugin_video_antiktv/atk_client.py:517
msgid "Account is marked as inactive."
msgstr "Konto je označené ako neaktívne."

#: plugin_video_antiktv/atk_client.py:519
msgid "Unsupported login response code received from server"
msgstr "Zo servera prišla nepodporovaná odpoveď na príkaz login"

#: plugin_video_antiktv/atk_client.py:531
msgid "No username or password provided"
msgstr "Nezadané prihlasovacie menu a heslo"

#: plugin_video_antiktv/atk_client.py:636
msgid "Unknown channel type"
msgstr "Neznámy typ kanálu"

#: plugin_video_antiktv/atk_client.py:704
msgid "Failed to get stream URL"
msgstr "Nepodarilo sa získať URL adresu streamu"

#: plugin_video_antiktv/atk_client.py:709
msgid "Not available stream found for this channel"
msgstr "Pre tento program sa nenašiel žiaden stream"

#: plugin_video_antiktv/atk_loader.py:57
#, python-brace-format
msgid "Platform of your receiver is currently not supported. Please send log file {log_file} to {email} and we will try to add support for it ..."
msgstr "Platforma vášho prijímača momentálne nie je podporovaná. Prosím pošlite log súbor {log_file} na {email} a pokúsime sa pre neho pridať podporu ..."

