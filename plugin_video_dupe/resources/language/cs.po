#: plugin_video_dupe/dupe.py:77
msgid "Unexpected return code from server"
msgstr "Neočekávaný návratový kód ze serveru"

#: plugin_video_dupe/dupe.py:123
#, python-brace-format
msgid "Login failed with response code {code}"
msgstr "Přihlášení se nezdařilo s kódem odpovědi {code}"

#: plugin_video_dupe/dupe.py:126
msgid "Login failed - probably wrong username or password"
msgstr "Přihlášení se nezdařilo – pravděpodobně nesprávné uživatelské jméno nebo heslo"

#: plugin_video_dupe/provider.py:31
msgid "Login failed. Check your credentials. Without valid credentials you will be unable to watch any content."
msgstr "Přihlášení se nezdařilo. Zkontrolujte své přihlašovací údaje. Bez platných přihlašovacích údajů nebudete moci sledovat žádný obsah."

#: plugin_video_dupe/provider.py:39
msgid "Movies"
msgstr "Filmy"

#: plugin_video_dupe/provider.py:40
msgid "Series"
msgstr "Seriály"

#: plugin_video_dupe/provider.py:53 plugin_video_dupe/provider.py:62
msgid "All"
msgstr "Vše"

#: plugin_video_dupe/provider.py:57 plugin_video_dupe/provider.py:66
msgid "By genre"
msgstr "Podle žánru"

#: plugin_video_dupe/provider.py:137 plugin_video_dupe/provider.py:187
msgid "In order to play content you need to enter login credentials in addon's setting."
msgstr "Abyste mohli přehrávat obsah, musíte zadat přihlašovací údaje v nastavení doplňku."

#: plugin_video_dupe/provider.py:190
msgid "You have reached the limit and playback of another item is not available for you."
msgstr "Dosáhli jste limitu a přehrávání další položky není k dispozici."

#: plugin_video_dupe/provider.py:195
msgid "No supported stream provider found"
msgstr "Nenalezen žádný podporovaný poskytovatel streamů"

#: plugin_video_dupe/provider.py:208
msgid "Please select a stream"
msgstr "Vyberte prosím stream"

#: plugin_video_dupe/provider.py:231
msgid "This video stream is not available. Try to select different one."
msgstr "Tento videostream není k dispozici. Zkuste vybrat jiný."

#: plugin_video_dupe/provider.py:251
msgid "Subtitles"
msgstr "Titulky"

#: plugin_video_dupe/provider.py:253
msgid "Please select subtitles"
msgstr "Vyberte prosím titulky"

#: plugin_video_dupe/stream_provider.py:228
msgid "Your internet provider is blocking access to site containing this video."
msgstr "Váš poskytovatel internetového připojení blokuje přístup k webové stránce obsahující toto video."

msgid "Login name"
msgstr "Přihlašovací jméno"

msgid "Login password"
msgstr "Přihlašovací heslo"

msgid "Max watched items"
msgstr "Maximální počet viděných položek"

msgid "Enable subtitles support"
msgstr "Povolit podporu pro titulky"

