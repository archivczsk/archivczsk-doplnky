#: plugin_video_magiogo/provider.py:122
msgid "Special section"
msgstr "Špeciálna sekcia"

#: plugin_video_magiogo/provider.py:133
msgid "Here you can show and optionaly remove/unregister unneeded devices, so you can login on another one."
msgstr "Tu si môžete zobraziť a prípadne vymazať/odregistrovať zbytočné zariadenia, aby ste sa mohli znova inde prihlásiť."

#: plugin_video_magiogo/provider.py:134
msgid "Registered devices"
msgstr "Zaregistrované zariadenia"

#: plugin_video_magiogo/provider.py:135
msgid "With this button you can check if playback works OK or it gives some error from the server."
msgstr "Týmto tlačidlom môžete skontrolovať, či prehrávanie funguje alebo server vracia nejakú chybovú správu."

#: plugin_video_magiogo/provider.py:136
msgid "Check playback"
msgstr "Kontrola prehrávania"

#: plugin_video_magiogo/provider.py:149
msgid "Remove device!"
msgstr "Zmazať zariadenie!"

#: plugin_video_magiogo/provider.py:150
msgid "In menu you can remove device using Remove device!"
msgstr "V menu môžete zariadenie vymazať pomocou Zmazať zariadenie!"

#: plugin_video_magiogo/provider.py:160
#, python-brace-format
msgid "Device {device} was removed!"
msgstr "Zariadenie {device} bolo vymazané!"

#: plugin_video_magiogo/provider.py:162
msgid "Error"
msgstr "Chyba"

#: plugin_video_magiogo/provider.py:173
msgid "Playback failed:"
msgstr "Prehrávanie zlyhalo:"

#: plugin_video_magiogo/provider.py:175
msgid "No error occured during playback check. Playback should work."
msgstr "Pri kontrole prehrávania nenastala žiadna chyba. Prehrávanie by malo fungovať."

#: plugin_video_magiogo/provider.py:177
msgid "Can't check playback. There are no channels available."
msgstr "Nemôžem skontrolovať prehrávanie. Nie je dostupný žiadny kanál."

